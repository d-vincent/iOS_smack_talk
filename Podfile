# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'
use_frameworks!

def install_shared_pods
    # Pods for Smack Talk
    pod 'Firebase/Core'
    pod 'Firebase/Database'
    pod 'IQKeyboardManagerSwift'
end

target "Smack Talk" do
    install_shared_pods
end

